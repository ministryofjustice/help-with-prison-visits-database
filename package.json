{
  "name": "help-with-prison-visits-database",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ministryofjustice/help-with-prison-visits-database.git"
  },
  "engines": {
    "node": "16.x.x"
  },
  "scripts": {
    "start": "node ./index.js && npm run int_migrations && npm run ext_migrations",
    "int_migrations": "KNEX_CONFIG=intMigrations knex migrate:latest --env intMigrations --migrations-directory=./internal",
    "ext_migrations": "KNEX_CONFIG=extMigrations knex migrate:latest --env extMigrations --migrations-directory=./external"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "knex": "^0.95.11",
    "mssql": "^7.2.1"
  }
}
